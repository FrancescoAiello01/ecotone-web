- provide(:title, "Biodiversity Reports")
%h1 All Biodiversity Reports
%ul.indexing
  - @biodiversity_reports.each do |report|
    %li.photo_list
      .col-md-8
        %h3= link_to "Biodiversity Report - #{report.date.strftime("%B %d, %Y")}", report
        %p= "- #{report.user.name}"
- if logged_in?
  = link_to "Add Biodiversity Report", new_biodiversity_report_path, class: "btn btn-primary add-btn"
